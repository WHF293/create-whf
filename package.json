{
    "name": "create-whf",
    "version": "1.0.2",
    "type": "module",
    "license": "MIT",
    "author": "WangHaoFeng",
    "keywords": [
        "cli",
        "vite",
        "ts"
    ],
    "description": "一个用于快速创建基于 vite + ts 项目模板的简易脚手架",
    "bin": {
        "create-whf": "index.js"
    },
    "files": [
        "index.js",
        "template-*",
        "dist"
    ],
    "main": "index.js",
    "scripts": {
        "dev": "unbuild --stub",
        "build": "unbuild",
        "typecheck": "tsc --noEmit",
        "prepublishOnly": "npm run build",
        "preCommit": "simple-git-hooks"
    },
    "simple-git-hooks": {
        "commit-msg": "node scripts/verifyCommit.js"
    },
    "engines": {
        "node": "^14.18.0 || >=16.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/WHF293/create-whf.git"
    },
    "bugs": {
        "url": "https://github.com/WHF293/create-whf/issues"
    },
    "homepage": "https://github.com/WHF293/create-whf.git#readme",
    "devDependencies": {
        "@types/node": "^18.11.18",
        "@types/prompts": "^2.4.2",
        "kolorist": "^1.7.0",
        "prompts": "^2.4.2",
        "rollup": "^3.12.1",
        "simple-git-hooks": "^2.8.1",
        "unbuild": "^1.1.1"
    }
}
